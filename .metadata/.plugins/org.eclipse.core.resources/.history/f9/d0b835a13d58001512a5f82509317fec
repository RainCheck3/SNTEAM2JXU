function displayDate() {
    document.getElementById("demo").innerHTML = Date();
    
    function employee() {
		firstName = 'Jiaju',
		lastName = 'Xu',
		function netSalary() {
			alert('Total Salary');
		};
    }
    function employee() {
		this.firstName = 'Jiaju';
		this.lastName = 'Xu';
		this.netSalary = function() {
			return (this.firstName + ' ' + this.lastName);
		};
	}
	var emp = new employee();
	alert(emp.netSalary());
}